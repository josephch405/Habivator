{
   "manifest_version": 2,
   "background": {
      //"scripts": ["background.js"  ]
   },
   "name": "Habivator - build a habit!",
   "key": "",
   "short_name": "Habivator",
   "version": "0.72",
   "description": "Reach your goals - working out, reading, coding, anything - but first build a habit.",
   "browser_action": {
      "default_icon": "/img/icon.png",
      "default_popup": "popup.html",
      "default_title": "Make a habit!"
   },
   "permissions": [
      "storage",
      "notifications",
      "identity"
   ],
   "oauth2":{
      "client_id": "532461452327-g8f10db6gcitei3hl4cshuuherkfe1h5.apps.googleusercontent.com",
      "scopes": ["https://www.googleapis.com/auth/plus.login"]
   },
   "options_page": "options.html",
   "icons": {"128": "/img/icon.png"},
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "offline_enabled": true
}